
<% "#{@scores_array}" %>

<% names = [] %>
<% scores = [] %>

<% @scores_array.each do |scoreset| %>
	<% scoreset.each do |key, value| %>
	<% names << User.find(key).name %>
	<% scores << "#{value}%" %>
	<%end%>

<%end %> 

scores.

<% count = 0 %>
<% scores.each do |score| %>

	<%= "#{names[count]} #{score} <br>" %>
	<% count += 1 %>
<% end %>